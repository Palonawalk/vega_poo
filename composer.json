{
    "name": "crazy/vega_poo",
    "description": "Learn poo in php building a web framework",
    "type": "project",
    "license": "MIT",
    "version": "1.0.0",
    "keywords": [
        "php",
        "php8",
        "nextformation",
        "newyork",
        "web",
        "framework",
        "vega"
    ],
    "homepage": "https://www.github.com/Palonawalk/vega_poo",
    "autoload": {
        "psr-4": {
            "App\\": "src/",
            "VegaCore\\": "vegaCore"
        }
    },
    "authors": [
        {
            "name": "Palona",
            "email": "lainedaridan.as@gmail.com",
            "homepage": "https://www.github.com/Palonawalk/vega_poo",
            "role": "Trainer and Developer"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "symfony/var-dumper": "^6.3"
    },
    "require": {
        "php-di/php-di": "^7.0",
        "symfony/http-foundation": "^6.3",
        "vlucas/phpdotenv": "^5.5"
    }
}
